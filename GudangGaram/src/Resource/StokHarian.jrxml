	<queryString>
		<![CDATA[SELECT a.`IdBarang`, a.`NamaBarang`, `Stok Awal`, `Stok Awal KG`, `Beli` as 'Beli', `Jual` as 'Jual', `Mutasi` as 'Mutasi', `Bahan` as 'Bahan', `Hasil` as 'Hasil', `Sak` as 'Penyesuaian', ifnull(`Beli`,0)+ifnull(`Hasil`,0)-IF(ifnull(`Sak`,0) > 0, ifnull(`Sak`,0), 0) as 'Total Masuk', ifnull(`Jual`,0)+ifnull(`Mutasi`,0)+ifnull(`Bahan`,0)-IF(ifnull(`Sak`,0) < 0, ifnull(`Sak`,0), 0) as 'Total Keluar' FROM (

(SELECT `IdBarang`, `NamaBarang` FROM `tbmbarang` WHERE 1 ) a
    LEFT JOIN
(SELECT `IdBarang`, SUM(`Karung`) as 'Beli' FROM `tbpenerimaan` WHERE 1 AND `Tanggal` = $P{Tgl} GROUP BY `IdBarang`) b
    ON a.`IdBarang`=b.`IdBarang`
    LEFT JOIN
(SELECT `IdBarang`, SUM(`Jumlah`) as 'Jual' FROM `tbpenjualandetail`a JOIN `tbpenjualan`b ON a.`NoTransaksi`=b.`NoTransaksi` WHERE 1 AND `Tanggal` = $P{Tgl} AND b.`NoTransaksi` LIKE '%-PJ' GROUP BY `IdBarang`) c
    ON a.`IdBarang`=c.`IdBarang`
    LEFT JOIN
(SELECT `IdBarang`, SUM(`Jumlah`) as 'Mutasi' FROM `tbpenjualandetail`a JOIN `tbpenjualan`b ON a.`NoTransaksi`=b.`NoTransaksi` WHERE 1 AND `Tanggal` = $P{Tgl} AND b.`NoTransaksi` LIKE '%-SJ' GROUP BY `IdBarang`) d
    ON a.`IdBarang`=d.`IdBarang`
    LEFT JOIN
(SELECT `IdBarangBahan`, ROUND(SUM(`JumlahBahan`) * 10 ) / 10 as 'Bahan' FROM `tbpacking`a JOIN `tbmbarang`b ON a.`IdBarangBahan`=b.`IdBarang` WHERE 1 AND `Tanggal` = $P{Tgl} GROUP BY `IdBarangBahan`) e
    ON a.`IdBarang`=e.`IdBarangBahan`
    LEFT JOIN
(SELECT `IdBarangHasil`, SUM(`JumlahHasil`) as 'Hasil' FROM `tbpacking`a JOIN `tbmbarang`b ON a.`IdBarangHasil`=b.`IdBarang` WHERE 1 AND `Tanggal` = $P{Tgl} GROUP BY `IdBarangHasil`) f
    ON a.`IdBarang`=f.`IdBarangHasil`
    LEFT JOIN
(SELECT `IdBarang`, `Sak` FROM `tbpenyesuaian` WHERE 1 AND `Tanggal` = $P{Tgl} GROUP BY `IdBarang`) g
    ON a.`IdBarang`=g.`IdBarang`
    LEFT JOIN
(SELECT `IdBarang`, `NamaBarang`, SUM(`Stok`) as 'Stok Awal', SUM(`Stokkg`) as 'Stok Awal Kg' FROM (
SELECT `IdBarang`, `NamaBarang`, 0 as 'Stok', 0 as 'Stokkg' FROM `tbmbarang` WHERE 1
UNION ALL
SELECT a.`IdBarang`, `NamaBarang`, ifnull(`Karung`,0) AS 'Stok', ifnull(`NettoPelita`,0) AS 'Stokkg' FROM `tbpenerimaan`a JOIN `tbmbarang`b ON a.`IdBarang`=b.`IdBarang` WHERE 1 AND `Tanggal` < $P{Tgl}
UNION ALL
SELECT a.`IdBarang`, `NamaBarang`, ifnull(`Jumlah`*-1,0) AS 'Stok', ifnull(`Jumlah`*-1*`Satuan`,0) AS 'Stokkg' FROM `tbpenjualandetail`a JOIN `tbmbarang`b ON a.`IdBarang`=b.`IdBarang` JOIN `tbpenjualan`c ON a.`NoTransaksi`=c.`NoTransaksi` WHERE 1 AND `Tanggal` < $P{Tgl}
UNION ALL
SELECT  `IdBarangBahan`, `NamaBarang`, `JumlahBahan`*-1, `JumlahBahan`*-1*`Satuan` FROM `tbpacking`a JOIN `tbmbarang`b ON a.`IdBarangBahan`=b.`IdBarang` WHERE 1 AND `Tanggal` < $P{Tgl}
UNION ALL
SELECT  `IdBarangHasil`, `NamaBarang`, `JumlahHasil`, `JumlahHasil`*`Satuan` FROM `tbpacking`a JOIN `tbmbarang`b ON a.`IdBarangHasil`=b.`IdBarang` WHERE 1 AND `Tanggal` < $P{Tgl}
UNION ALL
SELECT a.`IdBarang`, `NamaBarang`, ifnull(`Sak`,0), ifnull(`Jumlah`*`Satuan`,0) FROM `tbpenyesuaian`a JOIN `tbmbarang`b ON a.`IdBarang`=b.`IdBarang` WHERE 1 AND `Tanggal` < $P{Tgl}
) as tbTemp GROUP BY `IdBarang`) h
	ON a.`IdBarang`=h.`IdBarang`

) ORDER BY a.`IdBarang`]]>
	</queryString>
